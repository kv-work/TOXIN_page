{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb/base",
    "plugin:fsd/all"
  ],
  "rules": {
    "no-underscore-dangle": "off",
    "lines-between-class-members": ["error", "always", { "exceptAfterSingleLine": true }],
    "class-methods-use-this": "off"
  },
  "plugins": [
    "fsd"
  ]
}